{
  "name": "apptension-react-sketch",
  "version": "1.0.0",
  "description": "",
  "skpm": {
    "main": "apptension-react-sketch.sketchplugin",
    "manifest": "src/manifest.json"
  },
  "scripts": {
    "preview": "skpm-build --watch --run --manifest ./src/manifests/bundle.json",
    "build:sketch": "yarn build:sketch:symbols && yarn build:sketch:boilerplate",
    "build:sketch:symbols": "skpm-build --manifest ./src/manifests/symbols.json",
    "build:sketch:boilerplate": "skpm-build --manifest ./src/manifests/boilerplate.json",
    "build:web": "webpack --config ./webLibrary.webpack.config.js"
  },
  "author": "Michal Przyszczypkowski <mprzyszczypkowski@apptension.com>",
  "license": "MIT",
  "devDependencies": {
    "@skpm/builder": "^0.5.16",
    "babel-eslint": "9.0.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-sketch": "^0.2.4",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "inquirer-directory": "^2.1.0",
    "plop": "^2.3.0",
    "react-dom": "^16.8.4",
    "react-native-web": "^0.11.0"
  },
  "dependencies": {
    "chroma-js": "^1.2.2",
    "prop-types": "^15.5.8",
    "ramda": "^0.26.1",
    "react": "16.4.0",
    "react-primitives": "^0.6.0",
    "react-sketchapp": "^3.0.0-beta.3",
    "react-test-renderer": "^16.3.2",
    "styled-components": "^4.1.3"
  }
}
